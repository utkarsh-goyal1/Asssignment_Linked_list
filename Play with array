import java.util.*;

public class Main {
    public static void main (String args[]) {
        Scanner sc=new Scanner (System.in);
        int t=sc.nextInt();
        for(int i=0;i<t;i++)
        {
            int n=sc.nextInt();
            int []A=new int[n];
            for(int j=0;j<A.length;j++)
            {
                A[j]=sc.nextInt();
            }
            System.out.println(1);
        }
    }
}
